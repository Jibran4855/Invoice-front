{
  "name": "invoice-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "prod": "react-scripts build && npm start",
    "dev": "npm-run-all --parallel watch:*",
    "watch:webpack": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@mui/material": "^5.5.1",
    "@reduxjs/toolkit": "^1.8.0",
    "antd": "^4.19.5",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "classnames": "2.2.6",
    "cors": "^2.8.5",
    "dateformat": "^4.5.1",
    "dotenv": "^8.2.0",
    "eslint-plugin-flowtype": "5.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "2.29.1",
    "mongoose": "5.9.27",
    "node-sass": "4.14.1",
    "node-sass-package-importer": "5.3.2",
    "nodemailer": "^6.4.18",
    "react": "17.0.1",
    "react-chartjs-2": "2.11.1",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.1",
    "react-modal": "^3.12.1",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "^5.3.0",
    "react-table": "^7.7.0",
    "react-to-pdf": "0.0.14",
    "react-toastify": "^8.2.0",
    "reactstrap": "8.9.0",
    "reactstrap-confirm": "^1.3.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.6",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "watchpack": "^1.7.4",
    "watchpack-chokidar2": "2.0.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "5.2.0",
    "jquery": "3.5.1"
  }
}
